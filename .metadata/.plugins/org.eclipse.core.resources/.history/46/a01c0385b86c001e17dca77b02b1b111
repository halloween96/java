package edu;

public class Test2 {

	public static void main(String[] args) {
		int depositAmount;
		depositAmount = 50000;
		boolean isMarried = true;
		char gender = 'F';
		gender = 'M';
		String greeting = "Good Morning";
		System.out.println(depositAmount + "\n" + isMarried  + "\t" + gender + " " + greeting);
	}

}